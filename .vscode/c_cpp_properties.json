{
  "configurations": [
    {
      "name": "Linux",
      "includePath": ["${workspaceFolder}/**"],
      "defines": [],
      "compilerPath": "/usr/bin/clang++",
      "cStandard": "c23",
      "cppStandard": "c++23",
      "compileCommands": "${workspaceFolder}/build/host/compile_commands.json"
    },
    {
      "name": "Jetson Nano",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/sysroot/jetson-t210/usr/include/**",
